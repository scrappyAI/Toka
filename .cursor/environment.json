{
  "agentCanUpdateSnapshot": true,
  "environment": {
    "RUST_BACKTRACE": "1",
    "RUSTFLAGS": "-C link-arg=-fuse-ld=lld",
    "CARGO_INCREMENTAL": "0",
    "CARGO_NET_RETRY": "10",
    "CARGO_NET_TIMEOUT": "60",
    "RUST_LOG": "info",
    "RUSTC_WRAPPER": "",
    "PATH": "/usr/local/cargo/bin:/usr/local/rustup/bin:${PATH}"
  },
  "tools": {
    "rust": {
      "version": "1.86",
      "components": ["clippy", "rustfmt", "rust-analyzer", "rust-src"],
      "targets": ["x86_64-unknown-linux-gnu", "aarch64-unknown-linux-gnu"]
    },
    "cargo": {
      "available": true,
      "features": ["workspace", "offline", "locked"]
    }
  },
  "workspace": {
    "type": "rust",
    "manifestPath": "Cargo.toml",
    "workspaceMembers": "auto-detect",
    "buildTarget": "auto",
    "features": {
      "autoFormat": true,
      "autoLint": true,
      "incrementalBuild": false
    }
  },
  "agentCapabilities": {
    "canRunCargo": true,
    "canRunRustc": true,
    "canAccessToolchain": true,
    "canModifyDependencies": true,
    "maxMemoryMB": 2048,
    "maxCpuCores": 2
  }
}